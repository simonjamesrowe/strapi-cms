{
  "kind": "collectionType",
  "collectionName": "skills",
  "info": {
    "name": "Skills"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "rating": {
      "type": "decimal",
      "max": 10,
      "min": 1
    },
    "description": {
      "type": "richtext"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "order": {
      "type": "integer"
    },
    "jobs": {
      "via": "skills",
      "collection": "job"
    },
    "blogs": {
      "via": "skills",
      "collection": "blog"
    }
  }
}
